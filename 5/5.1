#include <stdio.h>
#include <limits.h>

int main() {
    int n, k;
    printf("Введите размер массива: ");
    scanf("%d", &n);
    printf("Введите число k: ");
    scanf("%d", &k);

    int arr[n];
    printf("Введите элементы массива: ");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int min_pos = INT_MAX;
    int found_multiple = 0;

    for (int i = 0; i < n; i++) {
        if (arr[i] % k == 0 && !found_multiple) {
            found_multiple = 1;
        } else if (arr[i] > 0 && found_multiple && arr[i] < min_pos) {
            min_pos = arr[i];
        }
    }

    // Если не нашли кратного k, ищем с начала
    if (!found_multiple) {
        for (int i = 0; i < n; i++) {
            if (arr[i] > 0 && arr[i] < min_pos) {
                min_pos = arr[i];
            }
        }
    }

    if (min_pos == INT_MAX) {
        printf("Нет положительных элементов после первого кратного k\n");
    } else {
        printf("Минимальный положительный элемент: %d\n", min_pos);
    }

    return 0;
}
