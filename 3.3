#include <stdio.h>

int main() {
    int n;
    printf("Введите размер матрицы: ");
    scanf("%d", &n);

    int A[n][n], X[n];

    printf("Введите элементы матрицы:\n");
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &A[i][j]);
        }
    }

    // Расчет элементов массива X
   for (int i = 0; i < n; i++) {
        int a = 1; // Флаг, указывающий, что все элементы строки удовлетворяют условию
        for (int j = 0; j < n; j++) {
            if (A[i][j] > A[j][i]) {
                a = 0; // Если условие не выполняется, флаг сбрасывается
            }
        }
        X[i] = a;
    }

    // Расчет суммы элементов массива X
    int sum = 0;
    for (int i = 0; i < n; i++) {
        sum += X[i];
    }

    printf("Сумма элементов массива X: %d\n", sum);

    return 0;
}
