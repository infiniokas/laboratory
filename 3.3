#include <stdio.h>

int main() {
    int n;
    printf("Введите размер матрицы: ");
    scanf("%d", &n);

    int A[n][n], X[n];

    printf("Введите элементы матрицы:\n");
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &A[i][j]);
        }
    }

    // Расчет элементов массива X
    for (int i = 0; i < n; i++) {
        X[i] = 1; // Предполагаем, что условие выполняется
        for (int j = 0; j < n; j++) {
            if (A[i][j] > A[j][i]) {
                X[i] = 0;
                break; // Если условие не выполняется, прерываем внутренний цикл
            }
        }
    }

    // Расчет суммы элементов массива X
    int sum = 0;
    for (int i = 0; i < n; i++) {
        sum += X[i];
    }

    printf("Сумма элементов массива X: %d\n", sum);

    return 0;
}
